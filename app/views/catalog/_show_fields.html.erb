<% if marc_field?(@document) %>
  <dt class="blacklight-<%= "tmsobjects" %>"><%= "Referenced YCBA Works:" %></dt>
  <dd class="blacklight-<%= "tmsobjects" %>">
    <%= get_ycba_objects(@document) %>
  </dd>
<% end %>
<dt class="blacklight-<%= "export" %>"><%= "Export:" %></dt>
<% if marc_field?(@document) %>
  <dd class="blacklight-<%= "export" %>"><%= link_to("XML",get_export_url_xml(@document),target: :_blank) %></dd>
<% else %>
  <dd class="blacklight-<%= "export" %>"><%= link_to("XML",get_export_url_xml(@document),target: :_blank) %></dd>
<% end %>
<% if @document["has_image_ss"][0] == "available" %>
<dt class="blacklight-<%= "iiifmanifest" %>"><%= "IIIF Manifest:" %></dt>
<dd class="blacklight-<%= "iiifmanifest" %>"><%= link_to("JSON",get_manifest_from_document(@document),target: :_blank) %></dd>
<% end %>
