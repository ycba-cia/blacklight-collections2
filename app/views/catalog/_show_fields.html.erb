<% if marc_field?(@document) %>
  <dt class="blacklight-<%= "holdings" %>"><%= "Holdings:" %></dt>
  <dd class="blacklight-<%= "export" %>"><%= get_holdings(@document) %></dd>
<%end %>
<dt class="blacklight-<%= "export" %>"><%= "Export:" %></dt>
<% if marc_field?(@document) %>
  <dd class="blacklight-<%= "export" %>"><%= link_to("XML",get_export_url_xml(@document)) %></dd>
<% else %>
  <dd class="blacklight-<%= "export" %>"><%= link_to("XML",get_export_url_xml(@document)) %> | <%= link_to("RDF",get_export_url_rdf(@document)) %></dd>
<% end %>

